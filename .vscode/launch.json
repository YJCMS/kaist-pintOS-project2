{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Project2 userprog Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/userprog/build/kernel.o",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/userprog/build",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "miDebuggerServerAddress": "localhost:1234"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/root/pintos-kaist",
      "program": "/root/pintos-kaist/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}